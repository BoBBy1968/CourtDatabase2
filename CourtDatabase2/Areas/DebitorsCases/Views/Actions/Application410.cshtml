@model CaseActionsCreateViewModel

@{
    ViewData["Title"] = "Заявление за издаване на заповед за изпълнение на парично задължение по чл. 410 от ГПК";
    var id = this.TempData["ID"];
    var currentAction = 1;
}

<h1>@ViewData["Title"]</h1>


<a asp-area="DebitorsCases" asp-controller="Home" asp-action="Details" asp-route-id="@id" class="btn btn-info">Обратно</a>
<a asp-area="DebitorsCases" asp-controller="Actions" asp-action="CreateActionReport" asp-route-lawCaseId="@id" asp-route-legalActionId="@currentAction" class="btn btn-info">Създай действие</a>


